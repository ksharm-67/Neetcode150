class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        
        seen = []
        lenLongest = 0

        temp = 0
        for i in range(len(s)):
            if s[i] not in seen:
                temp += 1
                seen.append(s[i])
            else:
                lenLongest = max(lenLongest, temp)

                indDupe = seen.index(s[i])
                seen = seen[indDupe + 1:]
                seen.append(s[i])
                temp = len(seen)
        
        return max(temp, lenLongest)