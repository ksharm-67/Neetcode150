class Solution:
    def subsetsWithDup(self, nums: List[int]) -> List[List[int]]:
        def backtrack(i, curr):
            if i >= len(nums):
                res.append(curr[:])
                return

            curr.append(nums[i])
            backtrack(i + 1, curr)
            curr.pop()

            while i + 1 < len(nums) and nums[i] == nums[i + 1]:
                i += 1
            backtrack(i + 1, curr)
    
        nums.sort()
        res = []
        backtrack(0, [])
        return res
